          "_customHTML": "<section class=\"contacts1\" group=\"Contacts\" plugins=\"amp-main, amp-form, amp-mustache\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n    <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n    <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n    <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n    <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showImg\">\n    <header>Title</header>\n    <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n    <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n    <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n        <option value=\"1\">Left</option>\n        <option value=\"2\" selected=\"\">Center</option>\n        <option value=\"3\">Right</option>\n    </select> \n    <header>Form</header>  \n    <input type=\"checkbox\" title=\"Show Title\" name=\"showFormTitle\">\n    <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showFormSubtitle\" checked=\"\">\n    <input type=\"checkbox\" title=\"List\" name=\"showFormList\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"listCount\" min=\"1\" max=\"9\" step=\"1\" value=\"1\" condition=\"showFormList\"> \n            <input type=\"checkbox\" title=\"Markers/Icons\" name=\"stylizedCounters\" condition=\"showFormList\">\n            <input type=\"range\" inline=\"\" title=\"Width Marker\" name=\"widthImg\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"0.8\" condition=\"showFormList\">\n            <input type=\"checkbox\" title=\"Title List\" name=\"showFormListTitle\" condition=\"showFormList\">\n            <input type=\"checkbox\" title=\"Text List\" name=\"showFormListText\" checked=\"\" condition=